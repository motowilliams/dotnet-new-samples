{
  "author": "Not Sayed Ibrahim Hashimi",
  "classifications": [ "Web" ], 
  "name": "Not Sayed Starter Web",
  "identity": "SayedHa.StarterWeb",         
  "shortName": "notsayedweb",                 // You can create the project using this short name instead of the one above.
  "tags": {
    "language": "C#"                       // Specify that this template is in C#.
  },
  "preferNameDirectory":"true",
  "sourceName": "SayedHa.StarterWeb",       // Will replace the string 'SayedHa.StarterWe' with the value provided via -n.
  "symbols":{
    "CopyrightName": {
      "type": "parameter",
      "defaultValue": "John Smith",
      "replaces":"Not Sayed Ibrahim Hashimi"
    },
    "Title": {
      "type": "parameter",
      "defaultValue": "Hello Web",
      "replaces":"Sayed Web"
    },
    "EnableContactPage":{
      "type": "parameter",
      "dataType":"bool",
      "defaultValue": "false"
    },
    "BackgroundColor":{
      "type":"parameter",
      "datatype": "choice",
      "defaultValue":"aliceblue",
      "choices": [
        {
          "choice": "aliceblue",
          "description": "Alice Blue"
        },
        {
          "choice": "dimgray",
          "description":"dimgray"
        },
        {
          "choice":"skyblue",
          "description":"sb"
        }
      ],
      "replaces":"skyblue"
    }
  },
"sources": [
    {
      "modifiers": [
        {
          "condition": "(!EnableContactPage)",
          "exclude": [
            "Views/Home/Contact.cshtml"
          ]
        }
      ]
    }
  ]
}